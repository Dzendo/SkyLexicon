<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dinadurykina.skylexicon">
    <uses-permission android:name="android.permission.INTERNET" />

    <!--Здесь один application - launcher.SkyApplication и он грузится первым, выгружается последним-->
    <application
        android:name="com.dinadurykina.skylexicon.launcher.SkyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sky">

        <!--NoActionBar Обязательно для включения материал appBarConfiguration -->
        <activity
            android:name="com.dinadurykina.skylexicon.launcher.SkyActivity"
            android:theme="@style/Theme.Sky.NoActionBar">
            <!--android:exported="TODO"-->

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>


</manifest>
